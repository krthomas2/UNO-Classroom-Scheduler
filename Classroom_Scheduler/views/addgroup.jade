extends layout
block content
    title #{title}
    form(method='post', action='/addGroupData')
          p Select Group:
          select(name='group')
                    #classys
                        -each classy in classys
                            option(value = classy.Group_ID) #{classy.Course_Title + " " + classy.Group_ID}
          p Select class# to add to group:
          select(name='groupy')
                    #classys2
                        -each classy2 in classys2
                            option(value = classy2._id) #{classy2.Class_ID +" "+ classy2.Course_Title}
          input(type='submit', class="btn btn-danger")
